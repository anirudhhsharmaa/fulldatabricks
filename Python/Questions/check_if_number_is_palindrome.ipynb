   "source": [
    "x = 1234\n",
    "temp = x\n",
    "y = 0\n",
    "\n",
    "while temp > 0:\n",
    "    y = y * 10 + temp % 10\n",
    "    temp //= 10\n",
    "\n",
    "if x == y:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not Palindrome\")"
   ]
