   "source": [
    "x = 1634\n",
    "temp = x\n",
    "\n",
    "order = len(str((x)))\n",
    "\n",
    "sum = 0\n",
    "\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    cube = digit ** order\n",
    "    sum += cube\n",
    "    temp //= 10\n",
    "\n",
    "if x == sum:\n",
    "    print(f\"{x} is an Armstrong number.\")\n",
    "else:\n",
    "    print(f\"{x} is not an Armstrong number.\")"
   ]
